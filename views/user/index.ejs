<div class="table-responsive">
    <table class="table">
        <thead class="thead-dark">
        <tr>
            <th>No.</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Access Level</th>
            <th>Password</th>
            <th>Action</th>
        </tr>
        </thead>
        <tbody>
        <% count = 1 %>
        <% users.forEach((user) => { %>
            <tr>
                <td><%= count %></td>
                <td><%= user.firstName %></td>
                <td><%= user.lastName %></td>
                <td><%= user.email %></td>
                <td><%= user.accessLevel %></td>
                <td><%= user.password %></td>
                <td><a class="btn btn-primary" href="/api/user/<%=user.id%>">Details</a> </td>
            </tr>
        <% count++ }) %>
        </tbody>
    </table>
</div>