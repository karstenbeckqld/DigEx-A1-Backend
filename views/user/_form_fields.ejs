<div class="container">
    <div class="row">
        <div class="col">
            <div class="form-group">
                <label for="firstName">First Name</label>
                <input class="form-control" id="firstName" type="text" name="firstName" value="<%= user.firstName %>">
            </div>

            <div class="form-group">
                <label for="lastName">Last Name</label>
                <input class="form-control" id="lastName" type="text" name="lastName" value="<%= user.lastName %>">
            </div>

            <div class="form-group">
                <label for="email">Email</label>
                <input required class="form-control" id="email" type="text" name="email" value="<%= user.email %>">
                <div class="email error"></div>
            </div>

            <div class="form-group">
                <label for="accessLevel">Access Level</label>
                <input class="form-control" id="accessLevel" type="text" name="accessLevel" value="<%= user.accessLevel %>">
            </div>

            <div class="form-group">
                <label for="password">Password</label>
                <input required class="form-control" id="password" type="password" name="password">
                <div class="password error"></div>
            </div>
        </div>
    </div>
</div>